pipeline {
        agent any

        stages {
            stage('Chekout GIT'){
                steps {
                    echo 'Pulling...'
                        git branch: 'master' ,
                        url : 'https://github.com/bahaeddinetlili/5TWIN4-G7-Projet2.git'
                }
            }

            stage('MVN CLEAN') {
                steps{
                    sh 'mvn clean install';

                }

            }

             stage('MVN COMPILE') {
                steps{
                    sh 'mvn compile';

                }

            }


            stage('MVN SONARQUBE') {
                steps {
                    sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=sonar -Dmaven.test.skip=true';
                }
            }



}
        }

